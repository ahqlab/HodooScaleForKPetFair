<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoginButton">
        <attr name="bg" format="reference|integer" />
        <attr name="symbol" format="reference|integer" />
        <attr name="text" format="reference|string" />
        <attr name="textColor" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="CommonEditText">
        <attr name="title" format="reference|string" />
        <attr name="editTextResourceId" format="reference" />
        <attr name="titleSize" format="reference|float" />
        <attr name="errorMessageSize" format="reference|float" />
        <attr name="hint" format="reference|string" />
        <attr name="edtFocusble" format="boolean"/>
        <attr name="edtText" format="string"/>
    </declare-styleable>

    <declare-styleable name="WeightView">
        <attr name="sufFix" format="reference|string"/>
        <attr name="textSize" format="dimension" />
        <attr name="displayNum" format="reference|integer"/>
    </declare-styleable>
    <declare-styleable name="TopImageLayout">
        <attr name="topImage" format="reference|integer"/>
        <attr name="topImageMargin" format="reference|integer"/>
    </declare-styleable>

</resources>